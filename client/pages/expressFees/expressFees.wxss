.sectiondetail{
  width: 100%;
  display: flex;
  font-size: 30rpx;
  height: 80rpx;
  align-items: center;
  margin-left: 20rpx;
}
.section{
  display: flex;
}
.picker{
  width: 80rpx;
  height: 40rpx;
  min-height: 40rpx;
  border:2rpx solid #ccc;
  text-align: center;
  padding-bottom: 10rpx;
}
.picker1{
  width: 300rpx;
  margin-left: 10rpx;
  margin-right: 10rpx;
  
}
.list{
  width:100%;
  display: flex;
  justify-content: center;
  height: 80rpx;
  font-size: 30rpx;
  padding-left: 20rpx;
  border-bottom: 2rpx solid #ccc;
  align-items: center;
}
.list text{
  flex: 1;
  text-align: center;
}
.selectTitle{
 width: 100%;
 padding-left: 20rpx; 
 font-size: 30rpx;
 color: blue;
 display: flex;
 flex-wrap: wrap
}
.selectTitle text {
  margin:20rpx 30rpx 20rpx 0;
  display: flex
}

.price{
  width: 100%;
  padding-left: 36rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  font-size: 30rpx;
}
.file,.parcel{
   width: 100%;
   display: flex;
   height: 80rpx;
   align-items: center;
}
.file input,.parcel input{
  width: 80rpx;
  min-height: 40rpx;
  border: 2rpx solid #ccc;
  height: 40rpx;
}
.fristWeight,.nextWeight{
  width:50%;
  display: flex;
}
.submit{
  width: 30%;
  height: 60rpx;
  font-size:30rpx;
  line-height:60rpx;
  margin-top:20rpx;
}

.loopLine{
  width: 100%;
  font-size: 30rpx;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  padding-left: 20rpx;
  border-bottom: 4rpx solid #ccc;
}
.loopLine .picker{
  width: 150rpx;
}
.loopLine .center{
  height: 80rpx;
  line-height: 80rpx;
}
.loopLine .item{
  margin-right: 20rpx;
  height: 80rpx;
  line-height: 80rpx;
}