.content{
  background: rgb(240,240,240);
  height: auto;
  padding-bottom: 30rpx;
}
.nodata{
  width: 100%;
  text-align: center;
  font-size: 40rpx;
  height: 60rpx;
  line-height: 60rpx;
}
.add{
  position: fixed;
  width: 100%;
  text-align: center;
  line-height: 100rpx;
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background: rgb(240,240,240);
  left: 0px;
  bottom: 4rpx;
  border-radius: 10rpx;
}
.add image{
  width:45rpx;
  height: 45rpx;
  margin-right: 10rpx;
}
.tite{
  background: #fff;
  text-align: center; 
  font-size: 36rpx; 
  height: 100rpx;
  line-height: 100rpx;
  width: 100%;
  color: #000;
  border-bottom: 2rpx solid rgb(153,153,153);
}
.radio-group{
  display: flex;
  width: 100%;
  justify-content: center;
  flex-direction: column;
  align-items: center;
}

.radio{
  margin-top:30rpx;
  width: 94%;
  display: flex;
  border:1rpx solid rgb(240,240,240);
  border-radius: 12rpx;
  background: #fff;
}
.radio-group :last-child{
  
}
.submit{
  margin: 20rpx 0;
}
.radiodetail{
  display: flex;
  font-size: 26rpx;
  flex-direction: column;
  padding: 10rpx 0;
}
.name-phone{
  height: 60rpx;
  line-height: 60rpx;
}
.name{
  padding-left: 20rpx;
  color: #000;
  height: 60rpx;
  line-height: 60rpx;
}
.address{
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  padding-left: 20rpx;
  height: 60rpx;
  line-height: 60rpx;
  color:rgb(153,153,153)
}
.address text{
  height: 50rpx;
  line-height: 50rpx;
}
.send{
  height: 50rpx;
  line-height: 50rpx;
  font-size: 30rpx;
  min-height: 40rpx;
}
.marginleft{
  margin-left: 20rpx;
  color: rgb(102,102,102);
  height: 60rpx;
  line-height: 60rpx;
}
