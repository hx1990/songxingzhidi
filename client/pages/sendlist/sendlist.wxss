.content{
  background: #fff;
}
.nodata{
  width: 100%;
  text-align: center;
  font-size: 40rpx;
  height: 60rpx;
  line-height: 60rpx;
}
.add{
  width: 100%;
  text-align: center;
  line-height: 80rpx;
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.add image{
  width:45rpx;
  height: 45rpx;
  margin-right: 10rpx;
}
.tite{text-align: center; 
font-size: 36rpx; 
height: 40rpx;
line-height: 40rpx;
}
.radio{
  display: flex;
  padding:10rpx 0; 
  border-bottom:2rpx dashed #ccc;
}
.radio-group :last-child{
  border-bottom:none;
}
.submit{
  margin: 10rpx 0;
}
.radiodetail{
  display: flex;
  font-size: 30rpx;
  flex-direction: column;
}
.address{
  display: flex;
  flex-wrap: wrap;
  align-items: center;
}
.address text{
  height: 50rpx;
  line-height: 50rpx;
}
.send{
  height: 50rpx;
  line-height: 50rpx;
  font-size: 30rpx;
  min-height: 40rpx;
}
.marginleft{
  margin-left: 20rpx;
}

.top {
  display: flex;
  width: 100%;
  height: 200rpx;
  justify-content: space-between;
  align-items: center;
  margin-top: 2rpx;
  background: #fff;
}
.topleft {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left: 20rpx
}
.topleft .userPic{
  width: 100rpx;
  height: 100rpx;
  margin-right: 20rpx
}
.topleft .myname {
  font-size: 34rpx;
  
}
.topright image{
  width: 60rpx;
  height: 60rpx;
}